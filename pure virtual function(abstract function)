#include <iostream>
#include<conio.h>
#include<string.h>
using namespace std;
//pure virtual function
class one{
public:
    virtual void display()=0;
};

class two:public one{
public:
    void display(){
    cout<<"I AM IN TWO"<<endl;
    }

};


class three:public one{
public:
    void display(){
    cout<<"I AM IN Three"<<endl;
    }
};

int main(){
two obj2;
three obj3;
obj2.display();
obj3.display();
return 0;                                                                                                                                                                                                              

}
